<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <NeutralLanguage>es-HN</NeutralLanguage>
    <Owners>César Andrés Morgan</Owners>
    <PackageProjectUrl>https://github.com/TheXDS/Sneik.git</PackageProjectUrl>
    <Summary>Juego simple de Snake escrito en C#</Summary>
    <PackageTags>snake;game;c#</PackageTags>
    <Title>Sneik</Title>
    <RootNamespace>TheXDS.Sneik</RootNamespace>
    <LangVersion>Latest</LangVersion>
    <Version>1.1.0</Version>
    <Description>$Summary.</Description>
    <Copyright>Copyright (c) 2019-$([System.DateTime]::Today.Year) César Andrés Morgan</Copyright>
    <RepositoryUrl>https://github.com/TheXDS/Sneik.git</RepositoryUrl>
    <PackageReleaseNotes>Mejoras del motor de juego
Nuevos tipos de Chunk</PackageReleaseNotes>
    <PackageLicenseExpression>mit</PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>
